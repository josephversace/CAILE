{
  "Deployment": {
    "IsDevelopment": true,
    "RequireAuth": false
  },

  "Development": {
    "UseInMemoryQdrant": true,
    "SkipWslCheck": true,
    "MockExternalServices": false,
    "EnableSwagger": true,
    "EnableDetailedErrors": true
  },

  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Information",
      "Microsoft.AspNetCore": "Debug",
      "IIM": "Trace"
    }
  },

  "Jwt": {
    "Key": "development-only-key-not-for-production-use-123456789"
  }
}